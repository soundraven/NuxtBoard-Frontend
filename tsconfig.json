{
    "extends": "./.nuxt/tsconfig.json",
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "strict": true,
        "jsx": "preserve",
        "importHelpers": true,
        "esModuleInterop": true,
        "allowJs": true,
        "noEmit": true,
        "skipLibCheck": true,
        "types": ["@nuxt/types", "@nuxtjs/tailwindcss"]
    },
    "exclude": ["node_modules", "dist"]
}
